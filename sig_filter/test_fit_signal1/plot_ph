#!/usr/bin/gnuplot

set xlabel "noise/signal ratio"
set ylabel "phase"

plot\
 "w1r2pi.txt" using 1:6 with linespoints pt 6,\
 "w1r2.txt"   using 1:6 with linespoints pt 6,\
 "w1r1.txt"   using 1:6 with linespoints pt 6,\
 "w1r.5.txt"  using 1:6 with linespoints pt 6,\
 "w1r.25.txt" using 1:6 with linespoints pt 6,\
 "w2r1.txt"   using 1:6 with linespoints pt 6,\
0.1

pause -1