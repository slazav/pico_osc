#!/usr/bin/gnuplot

set xlabel "noise/signal ratio"
set ylabel "amplitude"

plot\
 "w1r2pi.txt" using 1:5 with linespoints pt 6,\
 "w1r2.txt"   using 1:5 with linespoints pt 6,\
 "w1r1.txt"   using 1:5 with linespoints pt 6,\
 "w1r.5.txt"  using 1:5 with linespoints pt 6,\
 "w1r.25.txt" using 1:5 with linespoints pt 6,\
 "w2r1.txt"   using 1:5 with linespoints pt 6,\
2

pause -1